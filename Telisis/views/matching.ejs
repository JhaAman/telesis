<link rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/meyer-reset/2.0/reset.min.css'><script src='https://cdnjs.cloudflare.com/ajax/libs/prefixfree/1.0.7/prefixfree.min.js'></script><link rel='stylesheet' href='https://fonts.googleapis.com/css?family=Armata'>
<style class="cp-pen-styles">.profile-details:before {
  content: "";
  display: table;
  clear: both;
}

* {
  boz-sizing: border-box;
}

html {
  background: #ed2553;
}
html:after {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  display: block;
  background-color: #ed2553;
  z-index: -1;
}

body {
  padding: 1.5em;
}

.title,
.profile-name {
  font-family: roboto, sans-serif;
  font-weight: 400;
  color: black;
  text-transform: uppercase;
}

.title {
  font-size: 3em;
  text-align: center;
  margin: 0 auto 1em;
  line-height: 0.875;
}
.title .char1,
.title .char8 {
  font-size: 1.3em;
  letter-spacing: -0.025em;
}

.profile-list {
  list-style: none inside;
  margin: 0 auto;
  max-width: 32em;
}

.profile-list--item {
  display: inline-block;
  width: 100%;
  background: white;
  padding: 1.75em;
  border: 0.300em solid black;
  margin-bottom: 1.5em;
  text-align: right;
  transition: transform 0.1s linear;
  border-radius: 25px;
}
.profile-list--item:hover, .profile-list--item:focus {
  transform: scale(1.025);
}

.profile-img {
  height: 4.5em;
  width: 4.5em;
  margin-right: 1.5em;
  float: left;
  display: block;
  border-radius: 50%;
  border: 0.1875em solid black;
}
@media screen and (min-width: 32em) {
  .profile-img {
    width: 6em;
    height: 6em;
  }
}

.profile-name {
  padding-top: 0.5em;
  font-size: 1.5em;
}
@media screen and (min-width: 24em) {
  .profile-name {
    font-size: 1.875em;
  }
}

.profile-quote {
  color: black;
  margin-top: 1.5em;
  font: 400 1em/1.6 "Armata", sans-serif;
}
</style></head><body>
<h1 id="title" class="title"><font color="white">Connect</font></h1>

<ul class="profile-list">
  <% for(var i = 0; i < results.length; i++) { %>
    <li class="profile-list--item">
        <header class="profile-details">
          <img src="<%= results[i].pictureUrl %>" alt="" class="profile-img" />
          <h3 class="profile-name"><%= results[i].username %></h3>
        </header>
        <blockquote class="profile-quote">
          <p>Match Score: <%= results[i].weight %>, Zip Code: <%= results[i].zip %>, Country: <%= results[i].country %></p>
        </blockquote>
      </li>
  <% } %>
</ul>
<script src='//static.codepen.io/assets/common/stopExecutionOnTimeout-41c52890748cd7143004e05d3c5f786c66b19939c4500ce446314d1748483e13.js'></script><script src='https://s3-us-west-2.amazonaws.com/s.cdpn.io/5175/lettering-latest.min.js'></script>
